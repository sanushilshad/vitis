{
  "db_name": "PostgreSQL",
  "query": "\n        SELECT \n            SUM(\n                CASE period\n                    WHEN 'half_day' THEN 0.5\n                    WHEN 'full_day' THEN 1.0\n                    ELSE 0.0\n                END\n            ) as count\n        FROM leave\n        WHERE sender_id = $1\n          AND date >= $2\n          AND date <= $3\n          AND type = $4\n          AND status != $5 AND status != $6\n          AND is_deleted = false \n          AND status !='rejected'\n          AND status !='cancelled'\n        ",
